{
  "compilerOptions": {
		"lib": [
			"ES2015",
			"DOM"
		],
    "module": "commonjs",
    "esModuleInterop": true,
		"target": "es5",
		"moduleResolution": "node",
		"outDir": "dist",
		"baseUrl": ".",
		"sourceMap": true,
		"downlevelIteration": true,
		"noImplicitAny": false,
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": [
		"src/**/*"
	]
}
